package main

import "fmt"

func main() {
	var day int
	fmt.Print("Enter the day\n")
	fmt.Scan(&day)
	switch day {
	case 1:
		print("Monday\n")
	case 2:
		print("Tuesday\n")
	case 3:
		print("Wednesday\n")
	case 4:
		print("Thursday\n")
	case 5:
		print("Friday\n")
	case 6:
		print("Saturday\n")
	case 7:
		print("Sunday\n")
	default:
		print("Invalid day")
	}

}

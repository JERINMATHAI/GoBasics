package main

import "fmt"

func main() {
	var marks float32
	fmt.Print("Enter your mark: \n")
	fmt.Scan(&marks)
	if marks >= 90 {
		print("A Grade")
	} else if marks >= 80 {
		print("B Grade")
	} else if marks >= 70 {
		print("C Grade")
	} else if marks >= 60 {
		print("D Grade")
	} else if marks >= 50 {
		print("E Grade")
	} else {
		print("Failed")
	}
}
